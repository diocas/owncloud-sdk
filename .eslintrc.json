{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
  },
  "globals": {
  },
  "plugins": [
    "jasmine"
  ],
  "overrides": [
    {
      "files": ["src/*.js", "src/*/*.js"],
      "rules": {
        "prefer-promise-reject-errors": "off"
      }
    }
  ]
}
